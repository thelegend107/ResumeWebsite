{
  "routes": [
    {
      "route": "/admin/*",
      "allowedRoles": [ "admin" ]
    },
    {
      "route": "/home",
      "redirect": "/"
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 401,
      "redirect": "/unauthorized"
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{css, svg, js, ts, png, jpeg, jpg, ico}"]
  }
}